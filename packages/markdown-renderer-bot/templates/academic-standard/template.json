{
  "name": "academic-standard",
  "title": "Academic Standard",
  "description": "Clean, professional academic journal template with structured author metadata and accessibility features",
  "cssTemplate": "",
  "engines": ["html", "latex", "typst"],
  "defaultEngine": "typst",
  "metadata": {
    "type": "academic",
    "responsive": true,
    "printOptimized": true,
    "features": {
      "citations": true,
      "crossReferences": true,
      "mathSupport": true,
      "figureSupport": true,
      "structuredAuthors": true,
      "schemaOrg": true,
      "accessibility": true,
      "citationHover": {
        "enabled": true,
        "links": ["doi", "googleScholar"]
      }
    }
  }
}

{
  "name": "@myorg/test-bot",
  "version": "1.0.0",
  "description": "A Colloquium bot for Test Bot functionality",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": ["colloquium", "bot", "academic"],
  "author": {
    "name": "Your Name",
    "email": "you@example.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/myorg/test-bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/myorg/test-bot.git"
  },
  "bugs": {
    "url": "https://github.com/myorg/test-bot/issues"
  },
  "colloquium": {
    "botId": "test-bot",
    "apiVersion": "1.0.0",
    "permissions": ["read_manuscript"],
    "category": "utility",
    "isDefault": false
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build && npm test && npm run lint"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@colloquium/types": "*",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@colloquium/types": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}